        function choujiang(id) {


            //loading带文字
            layer.open({
                type: 2
              , content: '加载中...'
            });

            $.ajax(
                {
                    type: 'get',
                    url: 'ChouJiang.ashx?id=' + id + '&ctype=1&callback?',
                    dataType: 'json',
                    jsonp: "callback",
                    success: function (dataRestul) {
                        if (dataRestul.meg == "1") {
                            //抽取奖品的结果
                            //信息框
                            layer.open({
                                content: dataRestul.zt
                              , btn: '确定'
                            });
                            return;
                        }
                        else {

                            //信息框
                            layer.open({
                                content: dataRestul.zt
                              , btn: '确定'
                            });
                            return;

                         
                        }

                        //关闭弹出框加载信息
                        layer.closeAll();

                    }
                });
        }



        //获取URL值
        function GetQueryValue(queryName) {
            var query = decodeURI(window.location.search.substring(1));
            var vars = query.split("&");
            for (var i = 0; i < vars.length; i++) {
                var pair = vars[i].split("=");
                if (pair[0] == queryName) { return pair[1]; }
            }
            return "";
        }


